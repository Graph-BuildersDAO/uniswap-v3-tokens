{
  "name": "uniswap-v3-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/graph-buildersdao/uniswap-v3-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "create-local": "graph create ianlapham/uniswap-v3 --node http://127.0.0.1:8020",
    "deploy-local": "graph deploy ianlapham/uniswap-v3 --debug --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "deploy": "graph deploy ianlapham/uniswap-v3-subgraph --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --debug",
    "deploy-dev": "graph deploy sommelier/uniswap-v3 --ipfs http://35.197.14.14:5000/ --node http://35.197.14.14:8020/ --debug",
    "deploy-staging": "graph deploy $THE_GRAPH_GITHUB_USER/$THE_GRAPH_SUBGRAPH_NAME /Uniswap --ipfs https://api.staging.thegraph.com/ipfs/ --node https://api.staging.thegraph.com/deploy/",
    "watch-local": "graph deploy ianlapham/uniswap-v3 --watch --debug --node http://127.0.0.1:8020/ --ipfs http://localhost:5001",
    "deploy-hosted": "graph build && graph deploy --product hosted-service ",
    "deploy-studio": "graph build && graph deploy --studio ",
    "prepare-build": "cross-env ts-node ./script/build",
    "deploy-all-studio": "cross-env ts-node ./script/deploy-studio",
    "deploy-all-hosted": "cross-env ts-node ./script/deploy-hosted",
    "deploy-single": "cross-env ts-node ./script/deploy-single",
    "deploy-single-hosted": "cross-env ts-node ./script/deploy-single-hosted"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.64.1",
    "@graphprotocol/graph-ts": "^0.32.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "fs-extra": "^11.1.0",
    "mustache": "^3.1.0",
    "prettier": "^1.18.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "yargs": "17.7.2"
  }
}
